<project name="Pdebuild bootstrap" default="assemble" basedir=".">
	<property file="build.properties" />
	<target name="assemble">
		<ant antfile="assemble.${featureToBuild}.p2.xml" dir="${buildDirectory}">
			<property name="buildLabel" value="3.5" />
			<property name="buildId" value="${buildID}" />
		</ant>
		<ant antfile="assemble.${featureToBuild}.group.group.group.xml"
		     dir="${buildDirectory}">
			<property name="buildLabel" value="3.5" />
			<property name="buildId" value="${buildID}" />
		</ant>
		<ant antfile="package.${featureToBuild}.group.group.group.xml"
		     dir="${buildDirectory}">
			<property name="buildLabel" value="3.5" />
			<property name="buildId" value="${buildID}" />
		</ant>
	</target>
	<target name="package">
		<ant antfile="package.${featureToBuild}.group.group.group.xml" dir="${buildDirectory}">
			<property name="buildLabel" value="3.5" />
			<property name="buildId" value="${buildID}" />
		</ant>
	</target>
</project>